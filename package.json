{
  "name": "fuzzysort",
  "version": "1.1.4",
  "description": "Fast SublimeText-like fuzzy search for JavaScript",
  "keywords": [
    "fuzzy",
    "search",
    "filter",
    "javascript",
    "sublime"
  ],
  "author": "farzher",
  "license": "MIT",
  "main": "dist/fuzzysort.cjs.js",
  "module": "dist/fuzzysort.esm.js",
  "browser": "dist/fuzzysort.umd.js",
  "type": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "copy-types": "cp src/index.d.ts dist/",
    "dev": "rollup -c -w",
    "test": "npm run build && mocha",
    "build:legacy": "uglifyjs fuzzysort.js -m -c -o fuzzysort.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/farzher/fuzzysort.git"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "rollup": "^2.7.6"
  }
}
